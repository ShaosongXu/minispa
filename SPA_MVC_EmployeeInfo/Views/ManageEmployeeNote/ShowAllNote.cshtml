<div class="container">
    <div class="panel panel-primary">
        <div class="panel-body">
            <table class="table" >
                <thead>
                    <tr>
                        <th>NoteID</th>
                        <th>Description</th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <tr data-ng-repeat="note in Notes">
                        <td style="width: auto">{{note.noteID}}</td>
                        <td style="width: auto">{{note.description}}</td>
                        <td><a ng-click="editNote(note.noteID)"><span class="glyphicon glyphicon-edit">Edit</span></a></td>
                        <td><a ng-click="deleteNote(note.noteID)"><span class="glyphicon glyphicon-trash"></span></a></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>      